{
  "name": "cookbook",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "toolbox": "react-toolbox-themr"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "history": "^4.6.1",
    "meteor-node-stubs": "~0.2.4",
    "node-sass": "^3.13.1",
    "react": "^15.5.4",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-mounter": "^1.2.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.12"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1"
  },
  "peerDependencies": {
    "react-toolbox": "^2.0.0-beta.8"
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER",
      "APP_BAR",
      "NAVIGATION",
      "LINK",
      "DRAWER",
      "LIST",
      "OVERLAY",
      "PROGRESS_BAR",
      "AUTOCOMPLETE",
      "INPUT",
      "TABLE",
      "CARD",
      "AVATAR",
      "CHIP",
      "SLIDER",
      "TIME_PICKER",
      "SNACKBAR",
      "SWITCH",
      "TOOLTIP",
      "DIALOG",
      "DROPDOWN",
      "CHECKBOX"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "assets/react-toolbox"
  }
}
